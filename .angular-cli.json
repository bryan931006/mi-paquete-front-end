{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "mi-paquete-front-end"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../src/assets/theme/_main.scss",
        "../src/assets/theme/_themes/orange-theme.scss",
        "../src/assets/theme/components/app.scss",
        "../src/assets/theme/_common/form/form.scss",
        "../src/assets/theme/_common/button/button.scss",
        "../src/assets/theme/_common/alert/alert.scss",
        "../src/assets/theme/_common/animations/animations.scss",
        "../src/assets/theme/_common/card/card.scss",
        "../src/assets/theme/_common/chart/chart.scss",
        "../src/assets/theme/_common/dropdown/dropdown.scss",
        "../src/assets/theme/_common/flex/flex.scss",
        "../src/assets/theme/_common/images-container/images-container.scss",
        "../src/assets/theme/_common/items-list/items-list.scss",
        "../src/assets/theme/_common/logo/logo.scss",
        "../src/assets/theme/_common/modal/modal-tabs.scss",
        "../src/assets/theme/_common/navigation/navigation.scss",
        "../src/assets/theme/_common/nprogress/nprogress.scss",
        "../src/assets/theme/_common/pagination/pagination.scss",
        "../src/assets/theme/_common/sameheight-items/sameheight-items.scss",
        "../src/assets/theme/_common/scrollbar/scrollbar.scss",
        "../src/assets/theme/_common/table/table.scss",
        "../src/assets/theme/_common/_styles/_mixins.scss",
        "../src/assets/theme/_common/_styles/layout.scss",
        "../src/assets/theme/_common/_styles/misc.scss",
        "../src/assets/theme/_common/_styles/typography.scss",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/popper.js/dist/umd/popper.min.js",
        "../node_modules/popper.js/dist/umd/popper.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../src/assets/theme/_common/animations/animations.js",
        "../src/assets/theme/_common/items-list/items-list.js"
        
       
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
